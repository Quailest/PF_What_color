<h2>プロフィール編集</h2>
<%= form_with model: @user,url: public_user_path, local:true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>

  <%= f.label :introduction %>
  <%= f.text_field :introduction %>


  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.submit "変更を保存" %>
<% end %>